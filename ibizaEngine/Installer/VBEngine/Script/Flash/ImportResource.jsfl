fl.outputPanel.clear();fl.trace("run VBImportResource.");var includeURI = fl.scriptURI;includeURI.replace("VBImportResource.jsfl", "VBjsflLibs/VBInclude.jsfl");fl.runScript(includeURI.replace);if(doc) {	//폴더를 검사하여 생성	if(doc.library.itemExists("image") == false) {		doc.library.newFolder("image");		fl.trace("image 라이브러리 폴더 생성");	}	if(doc.library.itemExists("center") == false) {		doc.library.newFolder("center");		fl.trace("center 라이브러리 폴더 생성");	}	if(doc.library.itemExists("_dynamic") == false) {		doc.library.newFolder("_dynamic");		fl.trace("_dynamic 라이브러리 폴더 생성");	}		//slice폴더안에 unit들 가져오기	var folderContents = FLfile.listFolder(sliceFileType(doc.pathURI) + "_slice");	for(var i = 0; i < folderContents.length; i++) {		//temp파일이라면 continue(예:.DS_Store)		if('.' == folderContents[i][0])			continue;					//네임생성		var sliceName = sliceFileType(folderContents[i]);		var imageName = "image/" + sliceName;		var centerName = "center/" + sliceName;				//만약 업데이트하려는 unit이 이미 존재한다면 이전 center좌표를 유지하기 위해 이전좌표 저장		var olderX = 0;		var olderY = 0;		var imageStatus = "import : ";		if(doc.library.itemExists(centerName)) {			imageStatus = "update : ";			doc.library.editItem(centerName);			olderX = doc.getTimeline().layers[0].frames[0].elements[0].x;			olderY = doc.getTimeline().layers[0].frames[0].elements[0].y;			//좌표저장후 아이템 삭제			doc.selectAll();			if(doc.selection.length)				doc.deleteSelection();			doc.library.deleteItem(imageName);		}		//Scene에서 unit import		doc.editScene(0);		doc.importFile(sliceFileType(doc.pathURI) + "_slice/" + folderContents[i]);		fl.trace(imageStatus + folderContents[i]);				//import후 Scene에서 아이템 삭제		doc.selectAll();		if(doc.selection.length)			doc.deleteSelection();		//import한 unit올바른 name으로 설정후 image폴더로 이동		doc.library.selectItem(folderContents[i]);		doc.library.renameItem(sliceName);		doc.library.moveToFolder("image", sliceName, true);				if(!doc.library.itemExists(centerName)) {			//방금전 처리한 unit에 대한 center symbol이 존재하지 않는다면 생성			doc.library.addNewItem("movie clip", centerName);			doc.library.editItem(centerName);		} else {			//방금전 처리한 unit에 대한 center symbol이 존재한다면 symbol안의 이전 unit삭제			doc.library.editItem(centerName);			doc.selectAll();			if(doc.selection.length)				doc.deleteSelection();		}		//center symbol에 unit붙히기		doc.library.addItemToDocument({x:0, y:0}, imageName);		doc.getTimeline().layers[0].frames[0].elements[0].x = olderX;		doc.getTimeline().layers[0].frames[0].elements[0].y = olderY;	}	fl.trace("complete.");} else {	fl.trace("error : please open fla file for obj file.");}